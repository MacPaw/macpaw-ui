{
  "name": "@macpaw/macpaw-ui",
  "version": "0.0.50",
  "main": "lib/ui.js",
  "scripts": {
    "dev": "next -p 1234",
    "build": "next build && next export",
    "lint": "eslint ./src",
    "types": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
    "icons": "npx @svgr/cli ./src/Icons/svg --out-dir ./src/Icons/jsx --svgo-config .svgorc.json && npx eslint ./src/Icons/jsx --fix",
    "lib:js": "babel src --out-dir ./lib --presets=@babel/env,@babel/preset-react",
    "lib:css": "sass ./src/ui.scss ./lib/ui.css",
    "lib": "yarn lib:css && yarn lib:js && yarn types",
    "deploy:s3": "deploy-aws-s3-cloudfront --non-interactive --source=out --bucket=ui.macpaw.com --destination=/ --distribution=E2NQDOZEC6GH2E"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@mdx-js/loader": "^1.6.4",
    "@next/mdx": "^9.4.2",
    "@svgr/cli": "^5.4.0",
    "clsx": "^1.1.1",
    "copyfiles": "^2.3.0",
    "deploy-aws-s3-cloudfront": "^3.2.4",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "next": "^9.4.2",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10"
  },
  "typings": "lib/ui.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT"
}
