{
  "name": "@macpaw/macpaw-ui",
  "version": "0.0.33",
  "main": "lib/ui.js",
  "scripts": {
    "dev": "npx start-storybook -p 6006",
    "build": "build-storybook -o out",
    "lint": "eslint ./src",
    "icons:jsx": "npx @svgr/cli -d icons/jsx icons/svg",
    "icons:jsx2es6": "babel icons/jsx --out-dir icons/jsx --presets=@babel/env,@babel/preset-react",
    "icons": "yarn icons:jsx && yarn icons:jsx2es6",
    "lib:js": "babel src --out-dir lib --presets=@babel/env,@babel/preset-react",
    "lib:css": "sass ./src/ui.scss ./lib/ui.css",
    "lib": "yarn lib:css && yarn lib:js",
    "deploy:s3": "deploy-aws-s3-cloudfront --non-interactive --source=out --bucket=ui.macpaw.com --destination=/ --distribution=E2NQDOZEC6GH2E"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-links": "^6.0.1",
    "@storybook/react": "^6.0.16",
    "@svgr/cli": "^5.4.0",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.1",
    "deploy-aws-s3-cloudfront": "^3.2.4",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3"
  },
  "typings": "src/ui.d.ts",
  "files": [
    "src",
    "lib",
    "icons"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT"
}
